
# ============================================
# Timezone Configuration
# ============================================

GENERIC_TIMEZONE=Europe/Moscow
TZ=Europe/Moscow


# ============================================
# Database Configuration (PostgreSQL)
# ============================================

POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_SCHEMA=public
POSTGRES_PORT=5432


# ============================================
# n8n 
# ============================================

# N8N_ENCRYPTION_KEY=
# N8N_USER_MANAGEMENT_DISABLED=false

N8N_PORT=5678
# N8N_PATH=/n8n/
N8N_HOST=localhost
N8N_PROTOCOL=http
# LocalTunnel subdomain для публичного доступа к n8n
# Если subdomain занят, localtunnel выдаст случайный URL (проверьте логи)
LOCALTUNNEL_SUBDOMAIN=n8n-webhook
# После запуска localtunnel URL будет: https://${LOCALTUNNEL_SUBDOMAIN}.loca.lt/
# Обновите N8N_WEBHOOK_URL на полученный публичный URL
N8N_WEBHOOK_URL=http://localhost:5678/

N8N_DB_POSTGRES_DATABASE=n8n
N8N_DB_POSTGRES_USER=n8n
N8N_DB_POSTGRES_PASSWORD=
N8N_DB_POSTGRES_SCHEMA=public

N8N_MONGODB_DATABASE=n8n
N8N_MONGODB_USER=n8n
N8N_MONGODB_PASSWORD=

# Execution timeout in seconds (default: -1 for unlimited)
N8N_EXECUTIONS_TIMEOUT=-1
# Maximum execution timeout in seconds
# EXECUTIONS_TIMEOUT_MAX=3600
N8N_RUNNERS_ENABLED=true

N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
# Basic authentication (optional, for additional security)
N8N_BASIC_AUTH_ACTIVE=false
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=your_password_here

# Log level: debug, info, warn, error
N8N_LOG_LEVEL=info
# Log output destination
N8N_LOG_OUTPUT=console

# TLS/SSL certificate validation
# Set to 0 to disable certificate validation (NOT RECOMMENDED for production)
# Only use for troubleshooting connection issues
# NODE_TLS_REJECT_UNAUTHORIZED=1

# Custom paths for n8n data (usually handled by Docker volumes)
# N8N_USER_FOLDER=/home/node/.n8n

# Enable workflow history (requires Enterprise license)
# N8N_VERSION_NOTIFICATIONS_ENABLED=false
# Enable telemetry (set to false to disable)
# N8N_DIAGNOSTICS_ENABLED=true
# Disable production workflow executions (for testing)
# EXECUTIONS_PROCESS=main

# ============================================
# Flowise
# ============================================

FLOWISE_DB_USER=flowise
FLOWISE_DB_PASSWORD=
FLOWISE_DB_NAME=flowise

FLOWISE_PORT=3000

FLOWISE_SECRETKEY=my_secret

FLOWISE_DEBUG=false
# API Keys for external services (if needed)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# COHERE_API_KEY=
# HUGGINGFACE_API_KEY=
# etc.


# ============================================
# MongoDB Configuration (File Storage)
# ============================================

MONGODB_PORT=27017
MONGODB_ROOT_USER=admin
MONGODB_ROOT_PASSWORD=
# ВАЖНО: Установите пароль для root пользователя!
# Если пароль пустой, MongoDB будет работать без аутентификации
MONGODB_DATABASE=files

# MongoDB connection string for applications
# Format: mongodb://[username]:[password]@mongodb:27017/[database]?authSource=admin
# Example: mongodb://admin:password@mongodb:27017/files?authSource=admin
# 
# Для подключения через DataGrip:
# - Host: localhost
# - Port: 27017
# - Database: files (или n8n)
# - Username: admin (или n8n)
# - Password: значение из MONGODB_ROOT_PASSWORD (или N8N_MONGODB_PASSWORD)
# - Authentication Database: admin (ВАЖНО! Всегда указывайте admin)



